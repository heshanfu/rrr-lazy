{
  "name": "rrr-lazy",
  "version": "0.12.1",
  "description": "A fork of react-lazy-load and add support for react-router, react-router-hook.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/rrr-lazy.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/rrr-lazy.min.js --config webpack.config.production.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/rrr-lazy.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "react",
    "react-router",
    "load",
    "lazy",
    "lazyload",
    "react-router-hook",
    "react-component",
    "reactjs"
  ],
  "author": [
    "Bin Hou <houbin217jz@gmail.com> (https://twitter.com/houbin217jz)",
    "Jason Brown <loktar00@gmail.com> (https://twitter.com/loktar00)"
  ],
  "contributors": [
    "Sergey Laptev <iamsergeylaptev@gmail.com> (https://twitter.com/iamsergeylaptev)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "eslint-config-airbnb-deps": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "estraverse-fb": "^1.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "eventlistener": "0.0.1",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.17.4",
    "react-addons-shallow-compare": "^15.4.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  },
  "optionalDependencies": {
    "react-router": "^3.0.2"
  }
}
