{
  "name": "rrr-lazy",
  "version": "0.12.0",
  "description": "A fork of react-lazy-load and add support for react-router, react-router-hook.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/rrr-lazy.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/rrr-lazy.min.js --config webpack.config.production.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/rrr-lazy.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "react",
    "react-router",
    "load",
    "lazy",
    "lazyload",
    "react-router-hook",
    "react-component",
    "reactjs"
  ],
  "author": [
    "Bin Hou <houbin217jz@gmail.com> (https://twitter.com/houbin217jz)",
    "Jason Brown <loktar00@gmail.com> (https://twitter.com/loktar00)"
  ],
  "contributors": [
    "Sergey Laptev <iamsergeylaptev@gmail.com> (https://twitter.com/iamsergeylaptev)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "estraverse-fb": "^1.3.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "async": "^2.0.1",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "eventlistener": "0.0.1",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.16.3",
    "react-addons-shallow-compare": "^15.3.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  },
  "optionalDependencies": {
    "react-router": "^2.8.1"
  }
}
